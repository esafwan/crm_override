    "feather-icons": "^4.28.0",
    "frappe-ui": "^0.1.66",
    "gemoji": "^8.1.0",
    "lodash": "^4.17.21",
    "mime": "^4.0.1",
    "pinia": "^2.0.33",
    "socket.io-client": "^4.7.2",
    "sortablejs": "^1.15.0",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "vue": "^3.4.12",
    "vue-router": "^4.2.2",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
